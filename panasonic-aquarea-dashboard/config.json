{
  "name": "Panasonic Aquarea Dashboard (TCP)",
  "version": "0.0.4",
  "slug": "panasonic_aquarea_dashboard_tcp",
  "description": "Connects to a TCP stream (e.g. RS485 bridge / Telnet) and prints the Panasonic Aquarea dashboard into the add-on log.",
  "arch": ["aarch64", "amd64", "armv7", "armhf", "i386"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "options": {
    "host": "10.80.30.70",
    "port": 23,
    "print_interval_sec": 5,
    "strip_ansi": true,
    "max_buffer_kb": 256,
    "socket_timeout_sec": 1,
    "timestamp_format": "%Y-%m-%d %H:%M:%S"
  },
  "schema": {
    "host": "str",
    "port": "int",
    "print_interval_sec": "int",
    "strip_ansi": "bool",
    "max_buffer_kb": "int",
    "socket_timeout_sec": "int",
    "timestamp_format": "str"
  },
  "host_network": false
}
